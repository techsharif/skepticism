{
  "name": "skepticism",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "skepticism",
    "manifest": "src/manifest.json",
    "main": "skepticism.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "start:once": "skpm-build --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.8.0",
    "@skpm/extract-loader": "^2.0.3",
    "css-loader": "^6.7.1",
    "html-loader": "^4.2.0"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "author": "techsharif <sharif.cse.hstu@gmail.com>",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sketch-module-web-view": "^3.5.1"
  }
}
